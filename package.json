{
  "name": "@bitovi/n8n-nodes-watsonx",
  "version": "0.1.0",
  "description": "N8N node to connect to WatsonX",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "Manuel",
    "email": "mpreston@bitovi.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bitovi/n8n-nodes-watsonx.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WatsonXApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Watsonx/LmChatWatsonX.node.js",
      "dist/nodes/AgentV2custom/AgentV2custom.node.js"
    ]
  },
  "devDependencies": {
    "@types/lodash": "^4.17.17",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.94.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.94.0"
  },
  "dependencies": {
    "@langchain/community": "^0.3.45",
    "lodash": "^4.17.21"
  }
}
